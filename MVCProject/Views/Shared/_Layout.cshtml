<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link type="text/css" rel="stylesheet" href="/Content/MVCStyling2.css" />
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>

    <title>@ViewBag.Title</title>
</head>
<body>

    <h1>Richard Larsson</h1>
    <div>
        @Html.ActionLink("Home", "Index", "Home")
        @Html.ActionLink("About", "About", "Home")
        @Html.ActionLink("Projects", "Projects", "Home")
        @Html.ActionLink("Contact", "Contact", "Home")
        @Html.ActionLink("Feverchecker", "FeverCheck", "Doctor")
        @Html.ActionLink("Guessing Game", "GuessingGame", "Guesser")
        @Html.ActionLink("Trusted People", "PersonIndex", "Ajax")

    </div>
    <div class="main">
        @RenderBody()
    </div>

    <footer>@DateTime.Now.ToString("yyyy/MM/dd") Richard Larsson</footer>
    
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
